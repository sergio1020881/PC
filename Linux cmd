wget https://download.01.org/gfx/RPM-GPG-GROUP-KEY-ilg
sudo apt-key add RPM-GPG-GROUP-KEY-ilg
sudo apt-get update
sudo cp /etc/lsb-release /etc/lsb-release.bk
sudo vim /etc/lsb-release
DISTRIB_ID=Ubuntu
DISTRIB_RELEASE=17.04
DISTRIB_CODENAME=zesty
DISTRIB_DESCRIPTION="Ubuntu 17.04"
sudo rm -f /etc/lsb-release
sudo cp /etc/lsb-release.bk /etc/lsb-release

wget --no-check-certificate https://download.01.org/gfx/RPM-GPG-GROUP-KEY-ilg;sudo apt-key add RPM-GPG-GROUP-KEY-ilg
sudo apt-get install ttf-ancient-fonts aptdaemon
wget -O intel32.deb https://download.01.org/gfx/ubuntu/16.10/main/pool/main/i/intel-graphics-update-tool/intel-graphics-update-tool_2.0.4_i386.deb
chmod 777 intel-graphics-update-tool_2.0.6_i386.deb
sudo dpkg -i intel-graphics-update-tool_2.0.6_i386.deb
sudo apt-get -f install
intel-linux-graphics-update-tool
sudo apt-get clean
sudo apt-get update && sudo apt-get upgrade
sudo dpkg --configure -a
sudo apt-get -f install
sudo cp vainfo_2.1.0+ds1-1_i386.deb /var/cache/apt/archives/vainfo_2.1.0+ds1-1_i386.deb
