sudo add-apt-repository ppa:c.falco/mame
sudo apt-get update
sudo apt-get install mame
mame
cd ~/.mame && mame -cc
gedit ~/.mame/mame.ini
mkdir  ~/.mame/nvram memcard roms inp comments sta snap diff
sudo nautilus /usr/local/share/games/mame/roms
